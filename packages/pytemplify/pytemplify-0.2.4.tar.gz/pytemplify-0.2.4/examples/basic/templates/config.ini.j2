# Configuration for {{ gg.project }}
# Version: {{ gg.version }}
# Generated by PyTemplify

[project]
name = {{ dd.project_name }}
author = {{ gg.author }}
version = {{ gg.version }}

[services]
{% for service in dd.services -%}
{{ service.name }}_port = {{ service.port }}
{{ service.name }}_enabled = true
{% if service.database -%}
{{ service.name }}_database = {{ service.database }}
{% endif -%}
{% endfor %}

[logging]
level = INFO
format = %%(asctime)s - %%(name)s - %%(levelname)s - %%(message)s
