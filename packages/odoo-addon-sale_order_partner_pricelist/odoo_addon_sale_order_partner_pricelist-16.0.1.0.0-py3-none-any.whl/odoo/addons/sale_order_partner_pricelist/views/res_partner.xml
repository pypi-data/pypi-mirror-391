<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="partner_type_membership" model="ir.ui.view">
        <field name="name">partner_type_membership.view_partner_form</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form" />
        <field name="priority">99</field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='property_product_pricelist']" position="after">
                <field name="membership_sale_line_id" />
            </xpath>
        </field>
    </record>

</odoo>
