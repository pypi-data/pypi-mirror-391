trait RearLeftWheelSuspension:
    rear_left_suspension becomes Vehicles.Suspensions.LinearSpring:
        attachment_connector: rear_left_wheel.connector
        chassis_connector: chassis.rear_left_connector
    chassis.rear_left_connector is Vehicles.Suspensions.Charges.SuspensionConnector:
        forward_vector: chassis.forward_vector
        up_vector: chassis.up_vector
