trait RearRightWheelSuspension:
    rear_right_suspension becomes Vehicles.Suspensions.LinearSpring:
        attachment_connector: rear_right_wheel.connector
        chassis_connector: chassis.rear_right_connector
    chassis.rear_right_connector is Vehicles.Suspensions.Charges.SuspensionConnector:
        forward_vector: chassis.forward_vector
        up_vector: chassis.up_vector
