trait FrontRightWheelSuspension:
    front_right_suspension becomes Vehicles.Suspensions.LinearSpring with Vehicles.Suspensions.Traits.Steering:
        attachment_connector: front_right_wheel.connector
        chassis_connector: chassis.front_right_connector
    chassis.front_right_connector is Vehicles.Suspensions.Charges.SuspensionConnector:
        forward_vector: chassis.forward_vector
        up_vector: chassis.up_vector
