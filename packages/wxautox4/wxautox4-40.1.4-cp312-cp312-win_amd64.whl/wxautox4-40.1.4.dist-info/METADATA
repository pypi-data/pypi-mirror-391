import time
import threading

def scheduled_message():
    """å®šæ—¶å‘é€æ¶ˆæ¯"""
    while True:
        current_time = time.strftime('%H:%M')
        if current_time == '09:00':  # æ¯å¤©9ç‚¹å‘é€
            wx.SendMsg('æ—©ä¸Šå¥½ï¼æ–°çš„ä¸€å¤©å¼€å§‹äº†ï¼', 'å·¥ä½œç¾¤')
            time.sleep(60)  # é¿å…é‡å¤å‘é€
        time.sleep(30)  # æ¯30ç§’æ£€æŸ¥ä¸€æ¬¡
        
# å¯åŠ¨å®šæ—¶ä»»åŠ¡
scheduler_thread = threading.Thread(target=scheduled_message, daemon=True)
