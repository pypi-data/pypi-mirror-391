        chat.SendMsg('æ”¶åˆ°å›¾ç‰‡ï¼Œæ­£åœ¨å¤„ç†...')
    elif msg.type == 'file':
        chat.SendMsg('æ”¶åˆ°æ–‡ä»¶ï¼Œå·²ä¿å­˜')
        
wx.AddListenChat('å®¢æœç¾¤', advanced_message_handler)
