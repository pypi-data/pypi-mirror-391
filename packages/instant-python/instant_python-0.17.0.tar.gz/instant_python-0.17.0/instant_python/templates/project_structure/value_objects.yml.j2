      type: file
      extension: .py
      template: exceptions/base_error.py
    - name: domain_error
      type: file
      extension: .py
      template: exceptions/domain_error.py
    - name: incorrect_value_type_error
      type: file
      extension: .py
      template: exceptions/incorrect_value_type_error.py
    - name: invalid_negative_value_error
      type: file
      extension: .py
      template: exceptions/invalid_negative_value_error.py
    - name: invalid_id_format_error
      type: file
      extension: .py
      template: exceptions/invalid_id_format_error.py
    - name: required_value_error
      type: file
      extension: .py
      template: exceptions/required_value_error.py
- name: value_objects
  type: directory
  python: True
  children:
    - name: usables
      type: directory
      python: True
      children:
        - name: string_value_object
          type: file
          extension: .py
          template: value_object/string_value_object.py
        - name: int_value_object
          type: file
          extension: .py
          template: value_object/int_value_object.py
        - name: uuid
          type: file
          extension: .py
          template: value_object/uuid.py
    - name: decorators
      type: directory
      python: True
      children:
        - name: validation
          type: file
          extension: .py
          template: value_object/validation.py
    - name: value_object
      type: file
      extension: .py
      template: value_object/value_object.py
    - name: aggregate
      type: file
      template: value_object/aggregate.py
      extension: .py